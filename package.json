{
  "name": "apollo-server-es6",
  "version": "0.0.1",
  "main": "./src/server.js",
  "dependencies": {
    "apollo-server": "^2.0.0-rc.6",
    "graphql": "^0.13.2",
    "lodash": "^4.17.10",
    "schemaglue": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-minify": "0.4.3",
    "eslint": "5.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.2",
    "husky": "0.14.3",
    "nodemon": "1.17.5",
    "prettier": "1.13.7",
    "pretty-quick": "1.6.0"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "dev": "cd src && nodemon ./server.js -e js,graphql,json --exec babel-node",
    "build": "yarn run clean && BABEL_ENV=production babel src -d dist --copy-files",
    "start": "cd dist && node ./server.js",
    "serve": "yarn build && yarn start",
    "precommit": "pretty-quick --staged"
  },
  "author": "David SÃ¶derberg",
  "license": "MIT"
}
