{
  "name": "apollo-server-es6",
  "version": "0.0.1",
  "main": "./src/server.js",
  "dependencies": {
    "apollo-server": "^2.14.2",
    "graphql": "^15.0.0",
    "lodash": "^4.17.19",
    "schemaglue": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-minify": "0.5.1",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "dev": "cd src && nodemon ./server.js -e js,graphql,json --exec babel-node",
    "build": "yarn run clean && BABEL_ENV=production babel src -d dist --copy-files",
    "start": "cd dist && node ./server.js",
    "serve": "yarn build && yarn start",
    "precommit": "pretty-quick --staged"
  },
  "author": "David SÃ¶derberg",
  "license": "MIT"
}
